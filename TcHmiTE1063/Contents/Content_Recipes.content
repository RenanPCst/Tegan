<div id="TcHmiButton_ActivateRecipe" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="60" data-tchmi-height-unit="px" data-tchmi-left="880" data-tchmi-left-unit="px" data-tchmi-text="Activate" data-tchmi-top="1082" data-tchmi-top-unit="px" data-tchmi-width="165" data-tchmi-width-unit="px" data-tchmi-word-wrap="true" data-tchmi-text-font-size="20" data-tchmi-text-font-weight="Bold">

<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton_ActivateRecipe.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "// === Activate Recipe JS Script ===",
          "// 1. Set the activate button flag in the PLC",
          "TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.bRecipeActivateButton%/s%', true, function(res) {",
          "    if (res.error !== TcHmi.Errors.NONE) {",
          "        alert('Failed to trigger activation!');",
          "        return;",
          "    }",
          "    // 2. Wait for PLC response: success or error",
          "    var intervalId = setInterval(function() {",
          "        TcHmi.Symbol.readEx2('%s%PLC1.GVL_HMI.bRecipeActivateDone%/s%', function(doneResult) {",
          "            TcHmi.Symbol.readEx2('%s%PLC1.GVL_HMI.bRecipeActivateError%/s%', function(errorResult) {",
          "                var done = doneResult.value === true;",
          "                var error = errorResult.value === true;",
          "",
          "                // If either response flag is set, show alert and reset flags",
          "                if (done || error) {",
          "                    clearInterval(intervalId);",
          "",
          "                    if (done) {",
          "                        alert(\"Recipe Activated with success!\");",
          "                    } else if (error) {",
          "                        alert(\"Recipe Activate Error!\");",
          "                    }",
          "",
          "                    // 3. Reset both PLC flags (set to false)",
          "                    TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.bRecipeActivateDone%/s%', false);",
          "                    TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.bRecipeActivateError%/s%', false);",
          "                }",
          "            });",
          "        });",
          "    }, 300); // Check every 300ms",
          "});",
          ""
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
