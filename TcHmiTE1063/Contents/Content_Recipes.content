<div id="TcHmiButton_SaveRecipe" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="60" data-tchmi-height-unit="px" data-tchmi-left="680" data-tchmi-left-unit="px" data-tchmi-text="Save" data-tchmi-top="1082" data-tchmi-top-unit="px" data-tchmi-width="165" data-tchmi-width-unit="px" data-tchmi-word-wrap="true" data-tchmi-text-font-size="20" data-tchmi-text-font-weight="Bold">

<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton_SaveRecipe.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "// Save Button ",
          "// 1. Capture values from HMI controls",
          "console.log('--- Start SaveRecipe JS ---');",
          "",
          "var methodNameCtrl = TcHmi.Controls.get('TcHmiTextbox_MethodName');",
          "var createdByCtrl  = TcHmi.Controls.get('TcHmiTextblock_CreatedBy');",
          "",
          "var methodName = methodNameCtrl ? methodNameCtrl.getText() : '';",
          "var createdBy  = createdByCtrl  ? createdByCtrl.getText()  : '';",
          "",
          "console.log('MethodName:', methodName, 'CreatedBy:', createdBy);",
          "",
          "// 2. Write Method Name and User to PLC (RecipeInfo struct)",
          "TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.stRecipeInfo::sMethodName%/s%', methodName, function(resMethod) {",
          "    if (resMethod.error !== TcHmi.Errors.NONE) {",
          "        alert('Failed to write method name!');",
          "        return;",
          "    }",
          "    TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.stRecipeInfo::sCreatedBy%/s%', createdBy, function(resUser) {",
          "        if (resUser.error !== TcHmi.Errors.NONE) {",
          "            alert('Failed to write user!');",
          "            return;",
          "        }",
          "",
          "        // 3. Get the grid data",
          "        var gridSolvents = TcHmi.Controls.get('TcHmiDatagrid_SolventAndTimes');",
          "        var gridVolumes  = TcHmi.Controls.get('TcHmiDatagrid_Volumes');",
          "        var gridOther    = TcHmi.Controls.get('TcHmiDatagrid_OtherParams');",
          "        if (!gridSolvents || !gridVolumes || !gridOther) {",
          "            alert('Missing datagrid!');",
          "            return;",
          "        }",
          "",
          "        var rowsSolvents = gridSolvents.getSrcData();",
          "        var rowsVolumes  = gridVolumes.getSrcData();",
          "        var rowsOther    = gridOther.getSrcData();",
          "",
          "        // 4. Write to PLC temp arrays (for commit in PLC program)",
          "        TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.aSolventAndTimesRows%/s%', rowsSolvents, function(res1) {",
          "            TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.aVolumeRows%/s%', rowsVolumes, function(res2) {",
          "                TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.aOtherParamsRows%/s%', rowsOther, function(res3) {",
          "                    // 5. Trigger the PLC save sequence",
          "                    setTimeout(function() {",
          "                        TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.bRecipeSaveButton%/s%', true);",
          "                        console.log('SaveRecipe: Write & trigger completed!');",
          "",
          "                        // <Feedback handler> poll flags for result",
          "                        let pollCount = 0;",
          "                        const pollInterval = setInterval(function () {",
          "                            TcHmi.Symbol.readEx2('%s%PLC1.GVL_HMI.bRecipeSaveDone%/s%', function (resOk) {",
