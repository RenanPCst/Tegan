<div id="TcHmiButton_MovePumpsByVolume" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="40" data-tchmi-height-unit="px" data-tchmi-left="950" data-tchmi-left-unit="px" data-tchmi-text="Run By Volume" data-tchmi-top="821" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="true" data-tchmi-text-font-size="15">


<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton_MovePumpsByVolume.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "console.log(\"Button 'RunPumpsByVolume' pressed.\");",
          "",
          "let valid = true;",
          "let atLeastOneActive = false;",
          "let pumpData = [];",
          "",
          "// Loop through all 10 pumps",
          "for (let i = 0; i < 10; i++) {",
          "    const index = i + 1;",
          "",
          "    // Get control references",
          "    const volCtrl = TcHmi.Controls.get(`TcHmiNumericInput_volumePump${index}`);",
          "    const speedCtrl = TcHmi.Controls.get(`TcHmiNumericInput_speedPump${index}`);",
          "    const dirCtrl = TcHmi.Controls.get(`TcHmiCombobox_FwdOrBwdPump${index}`);",
          "    const chkCtrl = TcHmi.Controls.get(`TcHmiCheckbox_ActivatePump${index}`);",
          "",
          "    // Read values",
          "    const volume = volCtrl ? volCtrl.getValue() : 0;",
          "    const speed = speedCtrl ? speedCtrl.getValue() : 0;",
          "    const direction = dirCtrl ? dirCtrl.getSelectedValue() : null;",
          "    const rawState = chkCtrl ? chkCtrl.getToggleState() : 'Normal';",
          "    const active = (rawState === 'Active');",
          "",
          "    // Debug info",
          "    console.log(`Pump ${index} checkbox raw state: ${rawState}`);",
          "    console.log(`Pump ${index}: active=${active}, volume=${volume}, speed=${speed}, dir=${direction}`);",
          "",
          "    // Store info for later",
          "    pumpData.push({ active, volume, speed, direction });",
          "",
          "    if (active) {",
          "        atLeastOneActive = true;",
          "",
          "        // Validate volume, speed and direction",
          "        if (volume <= 0 || speed <= 0 || speed > 900 || direction == null) {",
          "            alert(`Pump ${index} has invalid volume, speed or direction.`);",
          "            valid = false;",
          "        }",
          "    }",
          "}",
          "",
          "// If no pump is selected, show alert",
          "if (!atLeastOneActive) {",
          "    alert('Please activate at least one pump before running the command.');",
          "}",
          "// If validation is OK, write values to PLC",
          "else if (valid) {",
          "    console.log(\"Validation OK. Writing values to PLC...\");",
          "",
          "    for (let i = 0; i < 10; i++) {",
          "        const { active, volume, speed, direction } = pumpData[i];",
          "        console.log(`* Pump ${i + 1} write: active=${active}, volume=${volume}, speed=${speed}, dir=${direction}`);",
          "",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::rVolume%/s%`, volume);",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::rSpeed%/s%`, speed);",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::eDirection%/s%`, direction);",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::bActive%/s%`, active);",
          "        //TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::bBusy%/s%`, active);",
          "    }",
          "",
          "    // Trigger the execution signal",
          "    TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.bRunPumpsByVolumeBt%/s%`, true);",
          "    console.log(\"Execution signal sent: bRunPumpsByVolumeBt = true\");",
          "}"
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
