<div id="TcHmiButton_MovePumpsBySpeed" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="40" data-tchmi-height-unit="px" data-tchmi-left="950" data-tchmi-left-unit="px" data-tchmi-text="Run By Speed" data-tchmi-top="761" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="true" data-tchmi-text-font-size="15">

<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton_MovePumpsBySpeed.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "console.log(\"Button 'RunPumpsBySpeed' pressed.\");",
          "",
          "let valid = true;",
          "let atLeastOneActive = false;",
          "let pumpData = [];",
          "",
          "// Loop through all 10 pumps",
          "for (let i = 0; i < 10; i++) {",
          "    const index = i + 1;",
          "",
          "    // Get control references",
          "    const speedCtrl = TcHmi.Controls.get(`TcHmiNumericInput_speedPump${index}`);",
          "    const dirCtrl = TcHmi.Controls.get(`TcHmiCombobox_FwdOrBwdPump${index}`);",
          "    const chkCtrl = TcHmi.Controls.get(`TcHmiCheckbox_ActivatePump${index}`);",
          "",
          "    // Read values",
          "    const speed = speedCtrl ? speedCtrl.getValue() : 0;",
          "    const direction = dirCtrl ? dirCtrl.getSelectedValue() : null;",
          "    const rawState = chkCtrl ? chkCtrl.getToggleState() : 'Normal';",
          "    const active = (rawState === 'Active');",
          "",
          "    // Debug info",
          "    console.log(`Pump ${index} checkbox raw state: ${rawState}`);",
          "    console.log(`Pump ${index}: active=${active}, speed=${speed}, dir=${direction}`);",
          "",
          "    // Store info for later",
          "    pumpData.push({ active, speed, direction });",
          "",
          "    if (active) {",
          "        atLeastOneActive = true;",
          "",
          "        // Validate speed and direction",
          "        if (speed <= 0 || speed > 900 || direction == null) {",
          "            alert(`Pump ${index} has invalid speed or direction.`);",
          "            valid = false;",
          "        }",
          "    }",
          "}",
          "",
          "// If no pump is selected, show alert",
          "if (!atLeastOneActive) {",
          "    alert('Please activate at least one pump before running the command.');",
          "}",
          "// If validation is OK, write values to PLC",
          "else if (valid) {",
          "    console.log(\"Validation OK. Writing values to PLC...\");",
          "",
          "    for (let i = 0; i < 10; i++) {",
          "        const { active, speed, direction } = pumpData[i];",
          "        console.log(`* Pump ${i + 1} write: active=${active}, speed=${speed}, dir=${direction}`);",
          "",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::rSpeed%/s%`, speed);",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::eDirection%/s%`, direction);",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::bActive%/s%`, active);",
          "        //TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::bBusy%/s%`, active);",
          "    }",
          "",
          "    // Trigger the execution signal",
          "    TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.bRunPumpsBySpeedBt%/s%`, true);",
          "    console.log(\"Execution signal sent: bRunPumpsBySpeedBt = true\");",
          "}"
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
