<div id="TcHmiButton_MoveValvesBt" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="40" data-tchmi-height-unit="px" data-tchmi-left="950" data-tchmi-left-unit="px" data-tchmi-text="Move Valves" data-tchmi-top="432" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="true" data-tchmi-text-font-size="15">

<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton_MoveValvesBt.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "console.log(\"Button 'MoveValves' pressed.\");",
          "",
          "let allValid = true;",
          "let valveValues = [];",
          "",
          "// Loop through all 5 valve controls",
          "for (let i = 1; i <= 5; i++) {",
          "    const combo = TcHmi.Controls.get(`TcHmiCombobox_Valve${i}Position`);",
          "",
          "    if (!combo) {",
          "        console.warn(`Valve ${i} combobox not found.`);",
          "        allValid = false;",
          "        break;",
          "    }",
          "",
          "    const selectedValue = combo.getSelectedValue();",
          "    console.log(`Valve ${i} selected value: ${selectedValue}`);",
          "",
          "    if (selectedValue === null || selectedValue === undefined || selectedValue <= 0) {",
          "        allValid = false;",
          "        break;",
          "    }",
          "",
          "    valveValues.push(selectedValue);",
          "}",
          "",
          "// Validation failed",
          "if (!allValid) {",
          "    alert(\"Please select a valid position for all 5 valves before proceeding.\");",
          "} else {",
          "    console.log(\"Validation OK. Writing values to PLC...\");",
          "",
          "    for (let i = 0; i < 5; i++) {",
          "        console.log(`â†’ Valve ${i + 1} write: pos=${valveValues[i]}`);",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualValvesCmd[${i}]::iTargetPos%/s%`, valveValues[i]);",
          "        TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualValvesCmd[${i}]::bMove%/s%`, true);",
          "    }",
          "}",
          ""
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
</div>
