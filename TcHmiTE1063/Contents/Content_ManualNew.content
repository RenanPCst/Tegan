<div id="TcHmiButton_MovePumpsByDistance" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="40" data-tchmi-height-unit="px" data-tchmi-left="950" data-tchmi-left-unit="px" data-tchmi-text="Run By Distance" data-tchmi-top="882" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="true" data-tchmi-text-font-size="15">


<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton_MovePumpsByDistance.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "console.log(\"Button 'RunPumpsByDistance' pressed.\");",
          "",
          "let valid = true;",
          "let atLeastOneActive = false;",
          "let pumpData = [];",
          "",
          "// Loop through all 10 pumps",
          "for (let i = 0; i < 10; i++) {",
          "    const index = i + 1;",
          "",
          "    // Get control references",
          "    const distCtrl = TcHmi.Controls.get(`TcHmiNumericInput_distancePump${index}`);",
          "    const speedCtrl = TcHmi.Controls.get(`TcHmiNumericInput_speedPump${index}`);",
          "    const dirCtrl = TcHmi.Controls.get(`TcHmiCombobox_FwdOrBwdPump${index}`);",
          "    const chkCtrl = TcHmi.Controls.get(`TcHmiCheckbox_ActivatePump${index}`);",
          "",
          "    // Read values",
          "    const distanceRaw = distCtrl ? distCtrl.getValue() : 0;",
          "    const distance = Math.floor(distanceRaw); // Ensure UINT-compatible value",
          "    const speed = speedCtrl ? speedCtrl.getValue() : 0;",
          "    const direction = dirCtrl ? dirCtrl.getSelectedValue() : null;",
          "    const rawState = chkCtrl ? chkCtrl.getToggleState() : 'Normal';",
          "    const active = (rawState === 'Active');",
          "",
          "    // Debug info",
          "    console.log(`Pump ${index} checkbox raw state: ${rawState}`);",
          "    console.log(`Pump ${index}: active=${active}, distance=${distance}, speed=${speed}, dir=${direction}`);",
          "",
          "    // Store info for later",
          "    pumpData.push({ active, distance, speed, direction });",
          "",
          "    // Validate if active",
          "    if (active) {",
          "        atLeastOneActive = true;",
          "",
          "        if (",
          "            distance <= 0 || distance > 65535 ||",
          "            speed <= 0 || speed > 900 ||",
          "            direction == null",
          "        ) {",
          "            alert(`Pump ${index} has invalid distance, speed or direction.`);",
          "            valid = false;",
          "            break;",
          "        }",
          "    }",
          "}",
          "",
          "// If no pump is selected, show alert",
          "if (!atLeastOneActive) {",
          "    alert('Please activate at least one pump before running the command.');",
          "} ",
          "// If validation is OK, write values to PLC",
          "else if (valid) {",
          "    console.log(\"Validation OK. Writing values to PLC...\");",
          "",
          "    for (let i = 0; i < 10; i++) {",
          "\t    const { active, distance, speed, direction } = pumpData[i];",
          "\t    const distSafe = Math.min(Math.max(parseInt(distance), 0), 65535);",
          "\t",
          "\t    console.log(`* Pump ${i + 1} write: active=${active}, distance=${distSafe}, speed=${speed}, dir=${direction}`);",
          "\t",
          "\t    TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::iDistance%/s%`, distSafe);",
          "\t    TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::rSpeed%/s%`, speed);",
          "\t    TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::eDirection%/s%`, direction);",
          "\t    TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.aManualPumpsCmd[${i}]::bActive%/s%`, active);",
          "\t}",
          "",
          "",
          "    // Trigger the execution signal",
          "    TcHmi.Symbol.writeEx(`%s%PLC1.GVL_HMI.bRunPumpsByDistanceBt%/s%`, true);",
          "    console.log(\"Execution signal sent: bRunPumpsByDistanceBt = true\");",
          "}"
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
