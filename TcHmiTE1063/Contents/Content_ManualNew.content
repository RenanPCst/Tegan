<div id="TcHmiButton_M1ManualRunBt" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="40" data-tchmi-height-unit="px" data-tchmi-left="30" data-tchmi-left-unit="px" data-tchmi-text="Run" data-tchmi-top="308" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="true" data-tchmi-text-font-size="15">

<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton_M1ManualRunBt.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "(function () {",
          "    let posControl = TcHmi.Controls.get(\"TcHmiCombobox_M1Position\");",
          "    if (!posControl) return;",
          "",
          "    let selectedIndex = posControl.getSelectedValue();",
          "",
          "    if (selectedIndex <= 0) {",
          "        TcHmi.Controls.get(\"TcHmiSystem\").showDialog({",
          "            title: \"Warning\",",
          "            message: \"Please select a valid position for Motor 1.\",",
          "            buttons: [{ text: \"OK\", result: \"ok\" }]",
          "        });",
          "        return;",
          "    }",
          "",
          "    // Write iPosition and bRun",
          "    TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.aManualMotorsCmd[0]::iPosition%/s%', selectedIndex, function (data) {",
          "        if (data.error === TcHmi.Errors.NONE) {",
          "            TcHmi.Symbol.writeEx('%s%PLC1.GVL_HMI.aManualMotorsCmd[0]::bRun%/s%', true);",
          "        }",
          "    });",
          "})();"
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
</div>
