<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiRectangle_BWasteLevel.onAttached",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "(function () {",
          "    const elementId = 'TcHmiRectangle_BWasteLevel'; ",
          "    const symbol = 'PLC1.GVL_HMI.HMIAnimation::rWasteLevel';",
          "    const maxHeight = 92; // <-- altura real do retângulo",
          "    const updateIntervalMs = 500;",
          "",
          "    const element = document.getElementById(elementId);",
          "    if (!element) {",
          "        console.warn('Element not found: ' + elementId);",
          "        return;",
          "    }",
          "",
          "    let intervalId = null;",
          "    let isUnloaded = false;",
          "",
          "    function updateLevel() {",
          "        if (isUnloaded) return;",
          "",
          "        TcHmi.Symbol.readEx2('%s%' + symbol + '%/s%', function (data) {",
          "            if (data.error === TcHmi.Errors.NONE) {",
          "                const value = data.value;",
          "",
          "                // Clamp 0–100 and convert to pixel height",
          "                const height = Math.min(Math.max(value, 0), 100) / 100 * maxHeight;",
          "",
          "                // Animate from bottom to top",
          "                const topInset = maxHeight - height;",
          "                element.style.clipPath = `inset(${topInset}px 0px 0px 0px)`;",
          "",
          "                // Inverted color logic for waste level",
          "                let color;",
          "                if (value > 70) {",
          "                    color = 'rgba(255, 0, 0, 0.6)'; // RED",
          "                } else if (value > 50) {",
          "                    color = 'rgba(255, 210, 0, 0.7)'; // YELLOW",
          "                } else {",
          "                    color = 'rgba(0, 150, 255, 0.6)'; // BLUE",
          "                }",
          "",
          "                element.style.backgroundColor = color;",
          "",
          "            } else {",
          "                console.error('Failed to read symbol:', symbol, data.error);",
          "            }",
          "        });",
          "    }",
          "",
          "    intervalId = setInterval(updateLevel, updateIntervalMs);",
          "    updateLevel();",
          "",
          "    if (TcHmi.EventProvider && TcHmi.EventProvider.register) {",
          "        TcHmi.EventProvider.register('TcHmi_Controls_System_TcHmiControl_Unloaded', function (e) {",
          "            if (e.control.getId() === elementId) {",
          "                clearInterval(intervalId);",
          "                isUnloaded = true;",
          "            }",
          "        });",
          "    }",
          "})();"
