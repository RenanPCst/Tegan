<div id="btnDownloadPDF_1" data-tchmi-type="TcHmi.Controls.Beckhoff.TcHmiButton" data-tchmi-height="50" data-tchmi-height-unit="px" data-tchmi-left="251" data-tchmi-left-unit="px" data-tchmi-text="Generate PDF" data-tchmi-top="368" data-tchmi-top-unit="px" data-tchmi-width="100" data-tchmi-width-unit="px" data-tchmi-word-wrap="True" data-tchmi-is-enabled="%s%ADS.PLC1.GVL_Report.bGeneratePDF2%/s%">
<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "%ctx%owner::Id|EventRegistrationMode=Resolve%/ctx%.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "// Função que usa a string lida do PLC",
          "function showMessageFromPLC(message) {",
          "    alert(\"Mensagem do PLC: \" + message);",
          "}",
          " ",
          "// Leitura da variável do PLC",
          "TcHmi.Symbol.readEx2('%s%ADS.PLC1.GVL_Report.sJsonOutput%/s%', function (data) {",
          "    if (data.error === TcHmi.Errors.NONE) {",
          "        let plcString = data.value; // valor retornado do PLC (string)",
          "        // Chamar a função passando a string",
          "        gerarRelatorioPDF(plcString);",
          "    } else {",
          "        console.error(\"Erro ao ler variável do PLC:\", data.error);",
          "    }",
          "});"
        ],
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
</div>
